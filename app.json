{
  "expo": {
    "name": "smanime",
    "slug": "smanime",
    "version": "1.0.0",
    "scheme": "smanime",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-full-room.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/images/logo-full.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo-full-room.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-full-room.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "562744b9-2a41-4c5e-a195-9f17411e84dc"
      }
    },
    "owner": "alaric987"
  }
}
